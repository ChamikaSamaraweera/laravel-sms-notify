{
    "name": "teaminfinitydev/laravel-sms-notify",
    "description": "Laravel Notifi.lk SMS Gateway Integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TeamInfinity Dev",
            "email": "info@teaminfinity.pro"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "TeamInfinityDev\\SmsNotify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamInfinityDev\\SmsNotify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TeamInfinityDev\\SmsNotify\\SmsNotifyServiceProvider"
            ],
            "aliases": {
                "SmsNotify": "TeamInfinityDev\\SmsNotify\\Facades\\SmsNotify"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}